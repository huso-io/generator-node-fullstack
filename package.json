{
  "name": "generator-node-fullstack",
  "version": "0.0.0",
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "scripts": {
    "pre-dev": "yarn install --force --check-files --har && yarn check-large-dirs",
    "start": "./node_modules/.bin/babel generators/**/*.es6 -d ./ -w",
    "build": "./node_modules/.bin/babel generators/**/*.es6 -d ./ && yarn check-large-dirs",
    "pretest": "./node_modules/.bin/eslint ./generators/**/*.es6 --fix",
    "check-large-dirs": "./node_modules/.bin/space-hogs",
    "security-check": "yarn lint && yarn flow && yarn check-package",
    "check-package": "./node_modules/.bin/nsp check && yarn validate && yarn check --integrity && yarn deps-license",
    "deps-license": "./node_modules/.bin/license-checker --production --csv --out licenses.csv && ./node_modules/.bin/license-checker --development --csv --out licenses-dev.csv",
    "validate": "yarn list --depth=0",
    "flow": "./node_modules/.bin/flow check",
    "lint": "./node_modules/.bin/jshint ./generators/**/*.es6 && ./node_modules/.bin/eslint ./generators/**/*.es6",
    "pre-release": "yarn check && sh ./tool/authors && ./node_modules/.bin/standard-version -a -n --prerelease alpha -i CHANGELOG.md && yarn pack && yarn check-large-dirs",
    "first-release": "yarn check && sh ./tool/authors && ./node_modules/.bin/standard-version --first-release -a -n -i CHANGELOG.md && yarn pack && yarn check-large-dirs",
    "release-major": "yarn check && sh ./tool/authors && ./node_modules/.bin/standard-version --release-as major -a -n -i CHANGELOG.md && yarn pack && yarn check-large-dirs",
    "release-minor": "yarn check && sh ./tool/authors && ./node_modules/.bin/standard-version --release-as minor -a -n -i CHANGELOG.md && yarn pack && yarn check-large-dirs",
    "release-patch": "yarn check && sh ./tool/authors && ./node_modules/.bin/standard-version --release-as patch -a -n -i CHANGELOG.md && yarn pack && yarn check-large-dirs",
    "update-atom-plugins": "apm upgrade --no-confirm"
  },
  "pre-commit": [
    "check-package",
    "validate"
  ],
  "dependencies": {
    "async": "^2.4.1",
    "chalk": "^1.1.3",
    "git-config": "^0.0.7",
    "glob": "^7.1.2",
    "lodash": "^4.17.4",
    "yeoman-generator": "^1.1.1",
    "yosay": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-jscript": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-xo-space": "^0.16.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "flow-bin": "^0.47.0",
    "jshint": "^2.9.4",
    "license-checker": "^11.0.0",
    "nsp": "^2.6.3",
    "precommit-hook": "^3.0.0",
    "space-hogs": "^1.5.0",
    "standard-version": "^4.0.0",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.6.0"
  },
  "engines": {
    "node": ">=7.10.0",
    "npm": ">=4.5.0",
    "yarn": ">=0.24.5"
  },
  "private": true,
  "keywords": [
    "yeoman-generator",
    "node-fullstack"
  ],
  "license": "Apache-2.0"
}
