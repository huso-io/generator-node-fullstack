{
  "name": "generator-dandan.com",
  "version": "0.0.0",
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "scripts": {
    "pre-dev": "yarn install --force --check-files --har",
    "start": "./node_modules/.bin/babel generators/**/*.es6 -d ./ -w",
    "build": "./node_modules/.bin/babel generators/**/*.es6 -d ./",
    "check-package-security": "nsp check",
    "pretest": "./node_modules/.bin/eslint ./generators/**/*.es6 --fix",
    "deps-license": "./node_modules/.bin/license-checker --production --csv --out licenses.csv && ./node_modules/.bin/license-checker --development --csv --out licenses-dev.csv",
    "lint": "./node_modules/.bin/eslint ./generators/**/*.es6",
    "validate": "yarn list --depth=0"
  },
  "pre-commit": [
    "check-package-security",
    "validate"
  ],
  "dependencies": {
    "chalk": "^1.1.3",
    "lodash": "^4.17.4",
    "yeoman-generator": "^1.1.1",
    "yosay": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-jscript": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^3.18.0",
    "eslint-config-xo-space": "^0.16.0",
    "eslint-plugin-flowtype": "^2.33.0",
    "license-checker": "^9.0.3",
    "nsp": "^2.6.3",
    "precommit-hook": "^3.0.0",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.6.0"
  },
  "engines": {
    "node": ">=7.10.0",
    "npm": ">=4.5.0",
    "yarn": ">=0.24.5"
  },
  "private": true,
  "keywords": [
    "yeoman-generator"
  ],
  "license": "Apache-2.0"
}
